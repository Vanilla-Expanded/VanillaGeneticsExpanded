<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Insectoids 2</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<GeneticRim.ExtractableAnimalsList>
							<defName>GR_TierTwoAnimalsInsectoidsVEI</defName>
							<tier>2</tier>
							<itemProduced>GR_InsectoidGenetic</itemProduced>
							<extractableAnimals>
								<li>VFEI2_Acidspitter</li>
								<li>VFEI2_Boomtick</li>
								<li>VFEI2_Durapod</li>
								<li>VFEI2_Empress</li>
								<li>VFEI2_Fuelmite</li>
								<li>VFEI2_Gigalocust</li>
								<li>VFEI2_Gigamite</li>
								<li>VFEI2_Hellbeetle</li>
								<li>VFEI2_Ironclad</li>
								<li>VFEI2_Macrofly</li>
								<li>VFEI2_Megapede</li>
								<li>VFEI2_Megathrips</li>
								<li>VFEI2_Megawasp</li>
								<li>VFEI2_Queen</li>
								<li>VFEI2_Silverfish</li>
								<li>VFEI2_Swarmling</li>
								<li>VFEI2_Tankroach</li>
								<li>VFEI2_Teramantis</li>
								<li>VFEI2_Titantick</li>
								<li>VFEI2_Venomite</li>
								
							</extractableAnimals>
						</GeneticRim.ExtractableAnimalsList>
					</value>
				</li>
				


				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName= "GR_Muffalopede"]/comps/li[@Name="GR_MuffalopedeJellyComp"]/resourceDef</xpath>
					<value>

						<resourceDef>VFEI2_RoyalInsectJelly</resourceDef>

					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName= "GR_Muffalopede"]/comps/li[@Name="GR_MuffalopedeChitinComp"]/woolDef</xpath>
					<value>

						<woolDef>VFEI2_Chitin</woolDef>

					</value>
				</li>


		






			</operations>
		</match>
	</Operation>


</Patch>