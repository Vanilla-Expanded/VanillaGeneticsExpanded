<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Erin's Mountain Animals</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesFeline"]/ingredients/li/filter/thingDefs</xpath>
					<value>
						<li>Corpse_Snow_leopard</li>
						<li>Corpse_Pallas_cat</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesFeline"]/fixedIngredientFilter/thingDefs</xpath>
					<value>
						<li>Corpse_Snow_leopard</li>
						<li>Corpse_Pallas_cat</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesRodent"]/ingredients/li/filter/thingDefs</xpath>
					<value>
						<li>Corpse_Pika</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesRodent"]/fixedIngredientFilter/thingDefs</xpath>
					<value>
						<li>Corpse_Pika</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<GeneticRim.ExtractableAnimalsList>
							<defName>GR_TierOneAnimalsFelineErin</defName>
							<tier>1</tier>
							<itemProduced>GR_FelineGenetic</itemProduced>
							<extractableAnimals>
								<li>Snow_leopard</li>
								<li>Pallas_cat</li>
							</extractableAnimals>
						</GeneticRim.ExtractableAnimalsList>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<GeneticRim.ExtractableAnimalsList>
							<defName>GR_TierOneAnimalsRodentErin</defName>
							<tier>1</tier>
							<itemProduced>GR_RodentGenetic</itemProduced>
							<extractableAnimals>
								<li>Pika</li>
							</extractableAnimals>
						</GeneticRim.ExtractableAnimalsList>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>