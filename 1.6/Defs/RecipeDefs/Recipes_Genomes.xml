<?xml version="1.0" encoding="utf-8" ?>
<Defs>






	
	<RecipeDef>
		<defName>GR_ExtractGenesBear</defName>
		<label>extract genes from an ursine</label>
		<description>Extract genetic material from an ursine corpse.</description>
		<jobString>Extracting Genetic Material</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>2500</workAmount>
		<workSkill>Intellectual</workSkill>
		<workSpeedStat>ResearchSpeed</workSpeedStat>
		<recipeUsers>
			<li>GR_GeneticExtractionTable</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>

					<thingDefs>
						<li>Corpse_Bear_Grizzly</li>
						<li>Corpse_Bear_Polar</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>Corpse_Bear_Grizzly</li>
				<li>Corpse_Bear_Polar</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_UrsineGenetic>1</GR_UrsineGenetic>
		</products>
		<skillRequirements>
			<Intellectual>6</Intellectual>
		</skillRequirements>
	</RecipeDef>

