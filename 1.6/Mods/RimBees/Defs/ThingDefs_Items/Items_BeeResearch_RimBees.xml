	<ThingDef Name="RB_ResearchPapersBaseGenetic" ParentName="RB_ResearchPapersBase" Abstract="True">
		<label>bee research papers (genetic)</label>
	</ThingDef>
	<ThingDef ParentName="ResourceBase">
		<defName>RB_RandomGeneticResearch</defName>
		<label>bee research (unfinished)</label>
		<description>This research is in progress.</description>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Things/Item/RB_BeeResearchNotes</texPath>
		</graphicData>
		<tradeability>None</tradeability>
		<statBases>
			<MaxHitPoints>5</MaxHitPoints>
			<Flammability>2.0</Flammability>
			<DeteriorationRate>6</DeteriorationRate>
			<Mass>10</Mass>
			<MarketValue>50</MarketValue>
		</statBases>
		<tickerType>Normal</tickerType>
		<thingCategories>
			<li>RB_BeeResearch</li>
		</thingCategories>
		<stackLimit>1</stackLimit>
		<comps>
			<li Class="RimBees.CompProperties_RandomResearch">
				<labelString>bee research papers (genetic)</labelString>
			</li>
		</comps>
	</ThingDef>
